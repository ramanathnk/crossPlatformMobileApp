{
  "expo": {
    "name": "IMDRA",
    "slug": "IMDRA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SnapTrackerLogoNew1024.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "IMDRA",
    "splash": {
      "image": "./assets/SnapTrackerLogoNew1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/SnapTrackerLogoNew1024.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ramanathnk.IMDRA"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "ndkVersion": "27.0.12077973"
          }
        }
      ],
      "expo-secure-store",
      "expo-font",
      "react-native-edge-to-edge"
    ],
    "extra": {
      "eas": {
        "projectId": "71602e3c-73c2-4558-8292-0e087065c848"
      }
    }
  }
}
